{
  "compilerOptions": {
      "module": "CommonJS",
      "target": "es2016",
      "lib": [
          "es2016"
      ],
      "sourceMap": true,
      "outDir": "lib",
      "rootDir": "src",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "types": [
          "node",
          "typescript/lib/tsserverlibrary"
      ]
  },
  "exclude": [
      "lib",
      "e2e",
      "node_modules",
      "test-workspace"
  ]
}